	date = {2001-06},
	note = {Conference Name: {IEEE} Transactions on Robotics and Automation},
	keywords = {Simultaneous localization and mapping, Mobile robots, Uncertainty, Navigation, Motion planning, Remotely operated vehicles, Environmental management, Helium, Mechatronics, Millimeter wave radar},
	file = {IEEE Xplore Abstract Record:/home/aa/Documents/T7/Zotero/storage/WRMUMMXL/stamp.html:text/html;IEEE Xplore Full Text PDF:/home/aa/Documents/T7/Zotero/storage/5G5QQVLG/Dissanayake et al. - 2001 - A solution to the simultaneous localization and ma.pdf:application/pdf},
