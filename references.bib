	file = {ScienceDirect Full Text PDF:/home/aa/Documents/T7/Zotero/storage/GTLHBV7S/De Bruyne and Werbrouck - 2018 - Merging self-driving cars with the law.pdf:application/pdf;ScienceDirect Snapshot:/home/aa/Documents/T7/Zotero/storage/GTBYNQ86/S0267364918300578.html:text/html},
}

