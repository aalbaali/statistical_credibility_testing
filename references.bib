	keywords = {Computer Science - Robotics},
	file = {arXiv Fulltext PDF:/home/aa/Documents/T7/Zotero/storage/P2BI3E4Q/Mangelson et al. - 2019 - Characterizing the Uncertainty of Jointly Distribu.pdf:application/pdf;arXiv.org Snapshot:/home/aa/Documents/T7/Zotero/storage/JEWXGKM3/1906.html:text/html;Mangelson et al. - 2020 - Characterizing the Uncertainty of Jointly Distribu.pdf:/home/aa/Documents/T7/Zotero/storage/95T4MV7Q/Mangelson et al. - 2020 - Characterizing the Uncertainty of Jointly Distribu.pdf:application/pdf},
