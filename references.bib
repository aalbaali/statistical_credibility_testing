	langid = {english},
	file = {Rosen et al. - 2016 - Towards lifelong feature-based mapping in semi-sta.pdf:/home/aa/Documents/T7/Zotero/storage/W8E268T2/Rosen et al. - 2016 - Towards lifelong feature-based mapping in semi-sta.pdf:application/pdf},
}

@inproceedings{meyer-deliusTemporaryMapsRobust2010,
	location = {Taipei},
	title = {Temporary maps for robust localization in semi-static environments},
	isbn = {978-1-4244-6674-0},
