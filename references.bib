	keywords = {Simultaneous localization and mapping, Trajectory, Cost function, optimisation, Robustness, {SLAM} (robots), graph theory, false positive loop closures, graph structure topological correctness, pose graph {SLAM} problem, probabilistic constraint graphs, probability, robust back-end optimizer, sensor-driven front-end, sparse optimization techniques, Switches},
	file = {IEEE Xplore Abstract Record:/home/aa/Documents/T7/Zotero/storage/UNYF6WKT/6224709.html:text/html;IEEE Xplore Full Text PDF:/home/aa/Documents/T7/Zotero/storage/SAAQHMRR/Sünderhauf and Protzel - 2012 - Towards a robust back-end for pose graph SLAM.pdf:application/pdf},
