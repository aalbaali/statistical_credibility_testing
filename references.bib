	date = {2015-10},
	note = {Conference Name: {IEEE} Transactions on Automatic Control},
	keywords = {Estimation, Equations, Kalman filters, Vectors, Covariance matrices, Noise, Additives, Unscented Kalman Filter ({UKF}), Unscented Transformation ({UT})},
	file = {IEEE Xplore Abstract Record:/home/aa/Documents/T7/Zotero/storage/JY92RA2Q/7042740.html:text/html;IEEE Xplore Full Text PDF:/home/aa/Documents/T7/Zotero/storage/KD4U39NJ/Menegaz et al. - 2015 - A Systematization of the Unscented Kalman Filter T.pdf:application/pdf},
