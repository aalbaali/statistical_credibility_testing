	keywords = {Computer Science - Robotics, 68T40, 74Pxx, 46N10, 65D19, Computer Science - Computer Vision and Pattern Recognition, G.1.6, I.2.9, I.4.5, Mathematics - Optimization and Control, 46N10, 65D19, 68T40, 74Pxx},
	file = {arXiv Fulltext PDF:/home/aa/Documents/T7/Zotero/storage/TYJWFE7S/Yang and Carlone - 2020 - One Ring to Rule Them All Certifiably Robust Geom.pdf:application/pdf;arXiv.org Snapshot:/home/aa/Documents/T7/Zotero/storage/PIBB5QDU/2006.html:text/html},
