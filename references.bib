	file = {IEEE Xplore Abstract Record:/home/aa/Documents/T7/Zotero/storage/DD6LTG75/8258997.html:text/html;IEEE Xplore Full Text PDF:/home/aa/Documents/T7/Zotero/storage/4I2UQNJJ/Vidal et al. - 2018 - Ultimate SLAM Combining Events, Images, and IMU f.pdf:application/pdf},
