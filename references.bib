	date = {2009-07-26},
	langid = {english},
	note = {Google-Books-{ID}: {jgEiuHlTCYcC}},
	keywords = {Mathematics / Applied, Mathematics / Algebra / General, Mathematics / Matrices},
	file = {aaed9c64ad7af8a6062a0de0b0c142a6eaaa.pdf:/home/aa/Documents/T7/Zotero/storage/PKY4DQFP/aaed9c64ad7af8a6062a0de0b0c142a6eaaa.pdf:application/pdf},
