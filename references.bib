	keywords = {Computer Science - Robotics},
	file = {arXiv Fulltext PDF:/home/aa/Documents/T7/Zotero/storage/GW3VJEQF/Rosen et al. - 2017 - SE-Sync A Certifiably Correct Algorithm for Synch.pdf:application/pdf;arXiv.org Snapshot:/home/aa/Documents/T7/Zotero/storage/Q52YZHAN/1612.html:text/html},
