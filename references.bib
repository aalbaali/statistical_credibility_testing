	urldate = {2021-02-07},
	date = {2021-03-01},
	langid = {english},
	keywords = {Motion planning, Behavior selection, Moving objects detection, Moving objects tracking, Obstacle avoidance, Occupancy grid mapping, Road mapping, Robot control, Robot localization, Route planning, Self-driving cars, Traffic signalization detection, Traffic signalization recognition},
	file = {ScienceDirect Full Text PDF:/home/aa/Documents/T7/Zotero/storage/5TR9ZY5P/Badue et al. - 2021 - Self-driving cars A survey.pdf:application/pdf;ScienceDirect Snapshot:/home/aa/Documents/T7/Zotero/storage/J89BZGYS/S095741742030628X.html:text/html},
