	langid = {english},
	keywords = {{SLAM}, Localization, {EKF}, Mapping, Mobile robot navigation},
	file = {ScienceDirect Full Text PDF:/home/aa/Documents/T7/Zotero/storage/86DN4KB9/Martinelli et al. - 2007 - A relative map approach to SLAM based on shift and.pdf:application/pdf;ScienceDirect Snapshot:/home/aa/Documents/T7/Zotero/storage/EFFGRFKU/S0921889006001473.html:text/html},
