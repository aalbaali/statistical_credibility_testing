	keywords = {Computer Science - Robotics, Robustness, {SLAM}, localization, mapping, simultaneous localization and mapping ({SLAM}), Localization, Factor graphs, Graph theory, maximum a posteriori estimation, perception, robots, sensing, Service robots, Simultaneous location and mapping},
	file = {arXiv Fulltext PDF:/home/aa/Documents/T7/Zotero/storage/T497Z827/Cadena et al. - 2016 - Past, Present, and Future of Simultaneous Localiza.pdf:application/pdf;arXiv.org Snapshot:/home/aa/Documents/T7/Zotero/storage/8WKA28BL/1606.html:text/html;IEEE Xplore Abstract Record:/home/aa/Documents/T7/Zotero/storage/C82D6XHB/7747236.html:text/html;IEEE Xplore Full Text PDF:/home/aa/Documents/T7/Zotero/storage/G3UFADMR/Cadena et al. - 2016 - Past, Present, and Future of Simultaneous Localiza.pdf:application/pdf},
